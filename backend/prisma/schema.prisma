generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"

}


model Game {
  id Int @id @default(autoincrement())
  startedAt DateTime 
  finishedAt DateTime
  elapsedTime Int

}

model Target{
  id Int @id 
  name String
  leftCoord Float
  topCoord Float
  width Float
  height Float
  iconPath String  // file path in supabase
}